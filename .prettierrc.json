{
	"plugins": ["prettier-sort-imports-plugin"],
	"arrowParens": "avoid",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"semi": true,
	"experimentalTernaries": false,
	"singleQuote": true,
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"trailingComma": "all",
	"singleAttributePerLine": true,
	"htmlWhitespaceSensitivity": "css",
	"proseWrap": "preserve",
	"insertPragma": false,
	"printWidth": 100,
	"requirePragma": false,
	"useTabs": true,
	"embeddedLanguageFormatting": "auto",
	"cursorOffset": 0,
	"tabWidth": 4
}
